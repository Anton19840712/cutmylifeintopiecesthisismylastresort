{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Error"
    }
  },

  "Server": {
    "Port": 8081,
    "Urls": "http://0.0.0.0:8081"
  },

  "Janus": {
    "ApiUrl": "http://janus:8088",
    "ProxyPath": "/janus"
  },

  "Cors": {
    "AllowAnyOrigin": false,
    "AllowedOrigins": [
      "https://yourdomain.com",
      "https://www.yourdomain.com"
    ],
    "AllowAnyMethod": true,
    "AllowAnyHeader": true
  },

  "SIP": {
    "Server": "sip:sip.linphone.org",
    "Proxy": "sip:sip.linphone.org:5060",
    "Username": "production-user",
    "Password": "CHANGE_ME_IN_PRODUCTION",
    "DisplayName": "Production User",
    "DestinationUri": "sip:destination@sip.linphone.org",
    "HangupDelay": 2000
  },

  "WebRTC": {
    "StunServers": [
      "stun:172.16.227.17:3478"
    ],
    "TurnServers": [
      {
        "urls": "turn:172.16.227.17:3478",
        "username": "webrtc",
        "credential": "webrtc123"
      }
    ],
    "IceTransportPolicy": "all",
    "OpusCodec": {
      "MinPtime": 10,
      "UseInbandFec": true,
      "MaxAverageBitrate": 64000,
      "Stereo": false,
      "Cbr": true
    }
  },

  "StaticFiles": {
    "RootPath": "wwwroot"
  }
}
