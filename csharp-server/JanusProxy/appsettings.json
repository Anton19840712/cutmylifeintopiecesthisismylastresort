{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "Server": {
    "Port": 8081,
    "Urls": "http://localhost:8081"
  },

  "Janus": {
    "ApiUrl": "http://localhost:8088",
    "ProxyPath": "/janus"
  },

  "Cors": {
    "AllowAnyOrigin": true,
    "AllowedOrigins": [],
    "AllowAnyMethod": true,
    "AllowAnyHeader": true
  },

  "Eureka": {
    "ServerUrl": "http://eureka-server:8761/eureka"
  },

  "ConfigService": {
    "ServiceName": "CONFIG-SERVICE"
  },

  "WebRTC": {
    "StunServers": [
      "stun:172.16.227.17:3478"
    ],
    "TurnServers": [
      {
        "urls": "turn:172.16.227.17:3478",
        "username": "webrtc",
        "credential": "webrtc123"
      }
    ],
    "IceTransportPolicy": "all",
    "OpusCodec": {
      "MinPtime": 10,
      "UseInbandFec": true,
      "MaxAverageBitrate": 64000,
      "Stereo": false,
      "Cbr": true
    }
  }
}
